{
    "manifest_version":3,
    "name":"OTP Autofiller",
    "version":"1.0.0", 
    "description":"Fetch otp your mailbox and autofill it",
   "action": {
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "identity"
  ],
  "host_permissions": ["<all_urls>"],
 
  "background": {
      "service_worker": "./src/background.js",
      "type": "module"
  },
  "web_accessible_resources": [{
    "resources": [
      "dist/audio/mixkit-bubble-pop-up-alert-notification-2357.wav",
      "dist/js/floatingBtn.js",
      "dist/css/floatingBtn.css",
      "dist/js/chunk-vendors.js"
    ],
    "matches": ["<all_urls>"]
  }],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "oauth2": {
    "client_id": "546267029640-heqet962tm1ek4005kpgcidum9q5rge0.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/gmail.modify"
    ]
  }
}