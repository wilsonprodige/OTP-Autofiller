{
    "manifest_version":3,
    "name":"OTP Autofiller",
    "version":"1.0.0", 
    "description":"Fetch otp your mailbox and autofill it",
   "action": {
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [{
    "resources": ["dist/js/floatingBtn.js"],
    "matches": ["<all_urls>"]
  }],
  "background": {
      "service_worker": "./src/background.js",
      "type": "module"
    }
}